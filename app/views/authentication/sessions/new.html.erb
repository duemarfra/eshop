<%= form_with(url: sessions_path) do |form| %>

  <div>
    <%= form.label :login %>
    <%= form.email_field :login, required: true %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password, required: true %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
